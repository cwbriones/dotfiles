{
    "coc.preferences" : {
        "formatOnSaveFiletypes" : []
    },
    "languageserver" : {
        "go" : {
            "command" : "$HOME/bin/gopls-wrapper.sh",
            "args" : [
                "-remote=auto",
                "-remote.listen.timeout=5m",
                "-logfile=auto",
                "-debug=:0",
                "-remote.debug=:0",
                "-rpc.trace"
            ],
            "rootPatterns" : ["go.mod", ".git"],
            "filetypes" : ["go"],
            "initializationOptions": {
                "staticcheck" : true,
                "analyses" : {
                    "ST1003" : false
                }
            },
            "analyzers.nonewvars" : true
        }
    },
    "diagnostic" : {
        "errorSign" : "✖︎",
        "warningSign" : "⚠︎"
    },
    "rust-analyzer.diagnostics.disabled" : [
            "unresolved-proc-macro"
    ],
    "rust-analyzer.rustfmt.overrideCommand" : "cargo +nightly fmt",
    "rust-analyzer.updates.channel" : "stable",
    "suggest.detailMaxLength" : 60,
    "python.pythonPath" : "/Users/cwbriones/.venvs/neovim/bin/python",
    "elixirLS.dialyzerEnabled": false
}
